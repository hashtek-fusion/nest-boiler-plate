{
  "name": "nest-boiler-plate",
  "version": "0.0.1",
  "description": "Boiler Plate code for NEST framework applications",
  "author": "Rajesh Ramamoorthy",
  "license": "MIT",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\"",
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "start:dev": "nodemon",
    "start:debug": "nodemon --config nodemon-debug.json",
    "prestart:prod": "rimraf dist && tsc",
    "start:prod": "node dist/main.js",
    "start:hmr": "node dist/server",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "webpack": "webpack --config webpack.config.js"
  },
  "dependencies": {
    "@nestjs/common": "^5.4.0",
    "@nestjs/core": "^5.4.0",
    "@nestjs/jwt": "^0.2.0",
    "@nestjs/mongoose": "^5.2.2",
    "@nestjs/passport": "^5.1.0",
    "@nestjs/swagger": "^2.5.1",
    "@types/auth0-js": "^8.11.6",
    "@types/aws-sdk": "^2.7.0",
    "@types/body-parser": "^1.17.0",
    "@types/cookie-parser": "^1.4.1",
    "@types/csurf": "^1.9.35",
    "@types/helmet": "0.0.42",
    "@types/passport-jwt": "^3.0.1",
    "@types/paypal-rest-sdk": "^1.7.2",
    "@types/underscore": "^1.8.9",
    "auth0-js": "^9.8.0",
    "aws-sdk": "^2.339.0",
    "body-parser": "^1.18.3",
    "class-transformer": "^0.1.10",
    "class-validator": "^0.9.1",
    "cluster": "^0.7.7",
    "cookie-parser": "^1.4.3",
    "csurf": "^1.9.0",
    "helmet": "^3.14.0",
    "mongoose": "^5.3.4",
    "os": "^0.1.1",
    "passport": "^0.4.0",
    "passport-jwt": "^4.0.0",
    "passport-oauth": "^1.0.0",
    "paypal-rest-sdk": "^1.8.1",
    "reflect-metadata": "^0.1.12",
    "rxjs": "^6.3.3",
    "typescript": "^3.1.3",
    "underscore": "^1.9.1"
  },
  "devDependencies": {
    "@nestjs/testing": "^5.4.0",
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.5",
    "@types/mongoose": "^5.2.19",
    "@types/node": "^10.12.0",
    "@types/supertest": "^2.0.5",
    "jest": "^23.5.0",
    "nodemon": "^1.18.3",
    "prettier": "^1.14.2",
    "rimraf": "^2.6.2",
    "supertest": "^3.1.0",
    "ts-jest": "^23.10.4",
    "ts-loader": "^4.4.2",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.5.0",
    "tslint": "5.11.0",
    "webpack": "^4.21.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
